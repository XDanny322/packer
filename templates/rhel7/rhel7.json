{
    "builders": [
        {
            "boot_command": [
                "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg<enter><wait>"
            ],
            "disk_size": "50000",
            "guest_os_type": "RedHat_64",
            "http_directory": "rhel7",
            "iso_checksum": "{{ user `checksum` }}",
            "iso_checksum_type": "md5",
            "iso_url": "{{ user `iso` }}",
            "shutdown_command": "/sbin/halt -p",
            "ssh_password": "packer",
            "ssh_port": 22,
            "ssh_username": "root",
            "ssh_wait_timeout": "600s",
            "type": "virtualbox-iso",
            "vboxmanage": [
                [
                    "modifyvm",
                    "{{.Name}}",
                    "--memory",
                    "2048"
                ],
                [
                    "modifyvm",
                    "{{.Name}}",
                    "--cpus",
                    "2"
                ],
                [
                    "modifyvm",
                    "{{.Name}}",
                    "--audio",
                    "none"
                ]
            ],
            "vm_name": "MyRHEL7"
        }
    ],
    "variables": {
        "checksum": "227880f6a3cee6b745e7f204586c8988",
        "iso": "http://192.168.86.223/test/rhel-server-7.4-x86_64-dvd.iso"
    }
}